{
	"width": 640,
	"height": 320,
	"fps": 30,
	"main": "./script/_bootstrap.js",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"parameterObject": {
			"type": "script",
			"path": "script/parameterObject.js",
			"global": true
		},
		"_bootstrap": {
			"type": "script",
			"path": "script/_bootstrap.js",
			"global": true
		},
		"tori": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/tori.png"
		},
		"play": {
			"type": "image",
			"width": 300,
			"height": 150,
			"path": "image/play.png"
		},
		"title": {
			"type": "image",
			"width": 640,
			"height": 320,
			"path": "image/title.png"
		},
		"result": {
			"type": "image",
			"width": 640,
			"height": 320,
			"path": "image/result.png"
		}
